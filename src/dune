(executables
  (names jsprover)
  (libraries prover.engine)
  (modes byte)
  (preprocess (pps js_of_ocaml-ppx)))

(rule
  (targets jsprover.js)
  (action
    (run %{bin:js_of_ocaml}
         +nat.js --opt 1 --source-map %{dep:jsprover.bc} -o %{targets})))

(alias
  (name default)
  (deps jsprover.js))
